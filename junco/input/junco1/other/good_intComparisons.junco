box main {
	prunt '< $;
	print ' , 3 < 4 $;
	print ' , 4 < 4 $;
	print ' , 5 < 4 $;
	print $;
	
	prunt '<, '= $;
	print ' , 3 <= 4 $;
	print ' , 4 <= 4 $;
	print ' , 5 <= 4 $;
	print $;
	
	prunt '> $;
	print ' , 3 > 4 $;
	print ' , 4 > 4 $;
	print ' , 5 > 4 $;
	print $;
		
	prunt '>, '= $;
	print ' , 3 >= 4 $;
	print ' , 4 >= 4 $;
	print ' , 5 >= 4 $;
	print $;
	
	
	prunt '=, '= $;
	print ' , 3 == 4 $;
	print ' , 4 == 4 $;
	print ' , 5 == 4 $;
	print $;
		
	prunt '<, '> $;
	print ' , 3 <> 4 $;
	print ' , 4 <> 4 $;
	print ' , 5 <> 4 $;
	print $;
	
}

