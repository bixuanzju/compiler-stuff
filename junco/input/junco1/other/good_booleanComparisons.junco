box main {
	
	prunt '=, '= $;
	print ' , true == true $;
	print ' , true == false $;
	print ' , false == true $;
	print ' , false == false $;
	print $;
		
	prunt '<, '> $;
	print ' , true <> true $;
	print ' , true <> false $;
	print ' , false <> true $;
	print ' , false <> false $;
	print $;
	
}

