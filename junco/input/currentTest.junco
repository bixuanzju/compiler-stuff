box main {
    const quarters <- 5;
    const dimes <- 3;
    const nickels <- 7;
    const pennies <- 17;
    const value <- quarters * 25 +
    			 dimes * 10 +
    			 nickels * 5 +
    			 pennies;
    print value $;
    
    const ncoins <- quarters + dimes + nickels + pennies;
	print ncoins $;
    
    const moredimes <- dimes > nickels;
    print moredimes;

    const boot <- true;
    const boof <- false;
    
    print boot, boof $;
    prunt boot, boof $;
}

